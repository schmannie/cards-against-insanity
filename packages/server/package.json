{
  "name": "cai-server",
  "version": "0.1.0",
  "main": "./dist/index.ts",
  "license": "MIT",
  "scripts": {
    "clean": "npx rimraf ./dist",
    "build": "npm run clean && npx tsc --outDir ./dist",
    "start::dev": "npx cross-env ROARR_LOG=true NODE_ENV=development npx nodemon ./src/index.ts | npx cross-env NODE_ENV=development npx roarr",
    "start::prod": "npm run build && npx cross-env ROARR_LOG=true node ./dist/index.js | npx roarr",
    "start": "npm run start::dev"
  },
  "dependencies": {
    "@roarr/cli": "^5.3.2",
    "@types/crypto-js": "^4.1.1",
    "cai-lib": "^0.1.3",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "rimraf": "^3.0.2",
    "roarr": "^7.11.0",
    "socket.io": "^4.4.1",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0"
  }
}
